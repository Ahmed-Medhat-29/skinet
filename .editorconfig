[*.cs]
csharp_style_prefer_primary_constructors = false
dotnet_diagnostic.IDE0290.severity = none

dotnet_naming_symbols.private_fields.applicable_kinds = field
dotnet_naming_symbols.private_fields.applicable_accessibilities = private

dotnet_naming_style.underscore_prefix.required_prefix = _
dotnet_naming_style.underscore_prefix.capitalization = camel_case

dotnet_naming_rule.private_fields_must_have_underscore.symbols = private_fields
dotnet_naming_rule.private_fields_must_have_underscore.style = underscore_prefix
dotnet_naming_rule.private_fields_must_have_underscore.severity = warning